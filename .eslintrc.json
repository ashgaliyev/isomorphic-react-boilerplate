{
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "es5",
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "parser": "flow",
      "semi": false
    }]
  }
}
